<%- include('partials/header') -%>
    <h1>Articles</h1>
    <div class="articles flex">
      <% if(articles.length) { %> <% articles.forEach(article => { %>
      <div class="article-title">
        <a href="/articles/<%= article.slug %>">
          <h3><%= article.title %></h3>
        </a>
      </div>
      <% }) %> <% } else {%>
      <h4>No Articles Present</h4>
      <% } %>
    </div>
  </body>
</html>